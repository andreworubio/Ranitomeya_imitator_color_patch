#Index genome

  ./STAR --runMode genomeGenerate --genomeDir /home/summerslab/ar_imitator_genome/STAR_index --genomeFastaFiles $HOME/imitator_genome/imitator.1.3.6.fa --runThreadN 24 --limitGenomeGenerateRAM 70744733397 --sjdbOverhang 149 --genomeChrBinNbits 18 --sjdbGTFfile /home/summerslab/imitator_genome/Ranitomeya_imitator.imitator.1.3.6.functional.gff3 --sjdbGTFtagExonParentTranscript Parent




